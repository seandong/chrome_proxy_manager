{
  "manifest_version": 2,
  "name": "Proxy Manager with Auth",
  "version": "1.0",
  "description": "Configure HTTP and SOCKS5 proxies with authentication support.",
  "permissions": [
    "proxy",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  }
}
